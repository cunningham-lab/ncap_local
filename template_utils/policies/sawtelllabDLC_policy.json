{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": "s3:GetBucketLocation",
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor3",
      "Effect": "Allow",
      "Action": "s3:HeadBucket",
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor1",
      "Effect": "Allow",
      "Action": "s3:ListAllMyBuckets",
      "Resource": "*"
    },
    {
      "Sid": "ListBucket",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::dlc-ncap"
      ],
      "Condition": {
        "StringEquals": {
          "s3:prefix": [
            "",
            "sawtelllabDLC/",
            "sawtelllabDLC/inputs",
            "sawtelllabDLC/results",
            "sawtelllabDLC/logs",
            "sawtelllabDLC/inputs/",
            "sawtelllabDLC/results/",
            "sawtelllabDLC/logs/"
          ],
          "s3:delimiter": [
            "/"
          ]
        }
      }
    },
    {
      "Sid": "ListSubBucket",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::dlc-ncap"
      ],
      "Condition": {
        "StringLike": {
          "s3:prefix": [
            "sawtelllabDLC/inputs/*",
            "sawtelllabDLC/results/*",
            "sawtelllabDLC/logs/*"
          ]
        }
      }
    },
    {
      "Sid": "Inputfolderwrite",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::dlc-ncap/sawtelllabDLC/inputs/*"
      ]
    },
    {
      "Sid": "Outputfolderwrite",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::dlc-ncap/sawtelllabDLC/results/*",
        "arn:aws:s3:::dlc-ncap/sawtelllabDLC/logs/*"
      ]
    }
  ]
}